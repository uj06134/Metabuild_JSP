<%@ page language="java" contentType="text/html; charset=UTF-8"
    pageEncoding="UTF-8"%>
<%
	request.setCharacterEncoding("UTF-8");
%>
<!-- MovieBean 객체 생성 -->
<jsp:useBean id="mb" class="myPkg.MovieBean"/>
<jsp:setProperty property="*" name="mb"/>
<% 
System.out.print(mb.getId());
%>
<%
	// genre(다수) 넣는법
	String[] ge = request.getParameterValues("genre");
	String str = "";
	for(int i=0; i<ge.length; i+=1){
		str += ge[i] + " ";
	}
	mb.setGenre(str);
%>

<!-- MovieDao 객체 생성 -->
<jsp:useBean id="mdao" class="myPkg.MovieDao"/>

<%
	int cnt = mdao.insertMovie(mb);
	if (cnt != -1){
		response.sendRedirect("select.jsp");
	}else {
		response.sendRedirect("insertForm.jsp");
	}
%>