package myPkg;

import java.sql.Timestamp;

public class ProductBean {
	private int id;
	private String name;
	private int price;
	private String company;
	private Timestamp regdate;
	
	public ProductBean() {
		
	}

	public ProductBean(int id, String name, int price, String company, Timestamp regdate) {
		super();
		this.id = id;
		this.name = name;
		this.price = price;
		this.company = company;
		this.regdate = regdate;
	}
	
	
	
}
